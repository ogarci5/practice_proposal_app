<div class="row">  
  <div class="span6 offset3">
    <h4 class="center">Log in</h4>
    <div class="log-in">
    <%= form_for(:session, url: sessions_path) do |f|%>
    <div>
      <%= f.label :username %>
      <%= f.text_field :username %>

      <%= f.label :password %>
      <%= f.password_field :password %>
      <br />
      <%= f.submit "Log in", class: "btn" %>
    </div>
    <% end %>
    </div>
  </div>
</div>
