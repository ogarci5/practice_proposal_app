<div class="row">  
  <div class="span6 offset3">
    <h4 class="center">Log in</h4>
    <%= form_for(:session, url: sessions_path) do |f|%>
      <%= f.label :username %>
      <%= f.text_field :username %>

      <%= f.label :password %>
      <%= f.text_field :password %>
      <br />
      <%= f.submit "Log in", class: "btn" %>
    <% end %>
  </div>
</div>
