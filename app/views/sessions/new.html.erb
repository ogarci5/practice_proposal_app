<h4 class="center">Log in</h4>
<div class="form">
<%= form_for(:session, url: sessions_path) do |f|%>
	<div>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>
    <br />
    <%= f.submit "Log in", class: "btn" %>
  </div>
<% end %>
</div>